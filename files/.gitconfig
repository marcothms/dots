[user]
	name = Marco Thomas
	email = mail@marco-thomas.net
[includeIf "gitdir:~/dev/lrz-gitlab/**"]
	path = "~/dev/lrz-gitlab/.gitconfig"
[alias]
	aa = add .
	st = status
	sh = show
	d = diff
	c = commit -v
	ca = commit -v --amend
	co = checkout
	b = branch
	pl = pull --rebase
	ps = push
	lg = log --oneline --decorate --graph
	yoink = pull
	yeet = push
